@import './lib/iconfont.wxss';

page {
  height: 100%;
  width: 100%;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  box-sizing: border-box;
}

.next_step {
  position: absolute;
  bottom: 3%;
  left: 0;
  right: 0;
  width: 670rpx;
  border-radius: 16rpx;
  color: #fff;
  background: linear-gradient(to right, #e306bf, #ad12b2, #60259f) !important;
  box-shadow: 0 0 2px #737271;
}

.pos-rel {
  position: relative;
}

.goods_info {
  padding: 40rpx 0;
  margin: 0 auto;
  width: 94%;
  overflow: hidden;
  font-size: 29rpx;
  line-height: 44rpx;
}

.img {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 240rpx;
  height: 200rpx;
  float: left;
  overflow: hidden;
  text-align: center;
}

.img image {
  width: 80%;
  height: 90%;
}

page {
  background: 483938;
}

#cover-preview {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 99999 !important;
  width: 100%;
  height: 100%;
  background-color: #000;
}

#croper {
  width: 600rpx;
  height: 600rpx;
  position: absolute;
  top: 300rpx;
  left: 75rpx;
  background-color: rgba(135, 206, 250, 0.5);
}

#croper-sure {
  width: 120rpx;
  height: 50rpx;
  border-radius: 10rpx;
  color: #000;
  background-color: rgba(135, 206, 250, 0.8);
  font-size: 40rpx;
  position: absolute;
  top: 946rpx;
  right: 10rpx;
  text-align: center;
}

#croper-cancel {
  width: 120rpx;
  height: 50rpx;
  border-radius: 10rpx;
  color: #000;
  background-color: rgba(135, 206, 250, 0.8);
  font-size: 40rpx;
  position: absolute;
  top: 946rpx;
  right: 150rpx;
  text-align: center;
}

.pack_top {
  position: relative;
  width: 100%;
  height: 760rpx;
  text-align: center;
  background-size: 100% 100%;
}

.scroll-view {
  overflow: hidden;
  white-space: nowrap;
  display: flex;
  width: 80%;
  margin: 0 auto;
  padding: 20rpx 60rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 99;
  opacity: 0.7;
}

.modalDlg {
  height: 796rpx;
  width: 700rpx;
  position: fixed;
  top: 4%;
  left: 0;
  z-index: 9999;
  margin: 0rpx 24rpx;
  background-color: #f1f0f5;
  border-radius: 16rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 26rpx;
  text-align: center;
}

.mt_title {
  display: block;
  width: 100%;
  color: #fff;
  background: linear-gradient(to right, #e306bf, #ad12b2, #60259f) !important;
  padding: 25rpx 0;
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
  text-indent: 20rpx;
  text-align: center;
  font-size: 30rpx;
  letter-spacing: 5rpx;
}

.zdy_text {
  width: 90%;
  border: 1px solid #c6c6c6;
  margin: 20rpx auto;
  border-radius: 10rpx;
  text-align: center;
  padding: 10rpx 0;
}

.zfy text {
  display: block;
  border: 1px solid #c6c6c6;
  margin: 20rpx auto;
  border-radius: 10rpx;
  text-align: center;
  color: #666;
  padding: 15rpx 0;
  background: #fff;
}

.modalDlg .next_step {
  bottom: -266rpx;
}

.zfy {
  width: 90%;
  margin: 0 auto;
}

.zfy i {
  color: #666;
}

.contact-box {
  position: absolute;
  right: 40rpx;
  bottom: 16%;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background: linear-gradient(to top, #df07be, #5f259f);
  text-align: center;
  opacity: 0.8;
}

.cont-btn {
  position: absolute;
  top: 0px;
  left: 0px;
  opacity: 0;
  width: 100%;
  height: 100%;
}
.cont-text {
  line-height: 10rpx;
  color: #fff;
  font-size: 24rpx;
}

.con-img {
  width: 50rpx;
  height: 50rpx;
  margin-top: 8rpx;
}

/*定制界面公共  */

/*祝语被选中以后  */

.act {
  color: #a314b0 !important;
  border-color: #a314b0 !important;
  box-shadow: 0 0 3rpx #a314b0;
}

/*纸套被选中  */

.activee {
  border-color: #a314b0 !important;
}

/*三个功能按钮  */

.pack_bottom {
  position: relative;
  width: 100%;
  height: 0;
  overflow: hidden;
  transition: all 400ms ease-in;
  background: #f1f0f5;
}

.gn_box {
  width: 100%;
  margin: 20rpx auto 0;
  text-align: center;
  color: #ededed;
  background: #fff;
  overflow: hidden;
  font-size: 32rpx;
  padding: 36rpx 0;
}

.gn_box p {
  display: block;
  color: #666;
}

.gn_box view {
  text-align: center;
  display: inline-block;
  width: 33.33%;
}

.gn_box view image {
  width: 89rpx;
  height: 64rpx;
  margin-bottom: 6rpx;
}

.canvas-box {
  position: absolute;
  left: -100000px;
  top: 0;
  z-index: -99;
  opacity: 0；

}
