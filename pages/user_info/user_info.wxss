/* pages/getuserInfo/user_info.wxss */

page {
  background: #eff0f2;
}

.im {
  width: 180rpx;
  height: 180rpx;
  margin: 150rpx auto 20rpx;
}

.bt {
  margin: 0rpx auto 100rpx;
  color: #666;
}

.ts {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 60rpx 80rpx;
  font-size: 32rpx;
  color: #666;
  border: 1px solid #bebaba;
  box-shadow: 0 0 3px #bebaba;
  background: #fff;
  border-radius: 10px;
}

.ty {
  margin-top: 200rpx;
  width: 400rpx;
  display: block;
  align-items: center;
  justify-content: center;
  padding: 0rpx 0;
  font-size: 32rpx;
  color: #fff;
  border: 1px solid #bebaba;
  box-shadow: 0 0 3px #bebaba;
  background: #a243b9;
}
/* 引导视频 */
.lead_video{
  width: 100%;height: 100%;
  position: fixed;
  top:0;
  left: 0;
  right: 0;
  bottom: 0
}
.lead_video video{
  width: 100%;
  height: 100%;
}
.close_video{
  position: fixed;
  top: 20rpx;
  right: 30rpx;
  color: #fff;
  font-size: 28rpx;
  z-index: 999;
}


