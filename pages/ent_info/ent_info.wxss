/* pages/ent_info/ent_info.wxss */

page {
  background: #eee;
  color: #4e4e4e;
}

.bg {
  width: 100%;
  background: #fff;
  padding: 30rpx 0;
  margin-bottom: 20rpx;
}
.bg2{padding: 20rpx 0}

.top {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #d74a3c;
}
.bg_top{margin-bottom: 20rpx}
.top view {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 10rpx;
}
.addr {
  width: 80%;
  overflow: hidden;
  margin-left: 15%;
}

.addr .add {
  font-size: 34rpx;
}

.addr_bg {
  width: 100%;
  background: white;
  margin: 20rpx 0;
}

.xx {
  padding: 20rpx 0;
}
.xx1{
  padding: 10rpx 0
}
.xx1 text{display: inline-block;white-space:normal;overflow: hidden}
.xx view {
  display: inline-flex;
  flex: auto (1 1 auto);
}

.time {
  font-size: 28rpx;
  color:#bab4b4 ;
}

.img image{height: 80%}

.tel {
  float: right;
}

.addr_bg1 {
  border-bottom: 1px solid #eee;
  padding-bottom: 20rpx;
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: 50rpx auto;
  background-position:40rpx 10rpx;
}
.addr_bg2 {
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: 50rpx auto;
  background-position:38rpx 80% ;
}
/*商品详情  */
.goods_list{background: #fff;font-size: 34rpx;margin-top: 24rpx;}
.goods_box{width: 94%;margin: 0 auto}
.bt_box{height: 130rpx;margin-left: 20rpx;justify-content:space-between;align-items:flex-start;flex-direction: column}
.title{display: flex;align-items: center;justify-content: space-between;padding: 0rpx 16rpx 0rpx}
.img{width: 180rpx;height: 180rpx;margin-right: 40rpx}
.goods_info{padding: 20rpx 0;width: 100%}
.bt{font-size:32rpx ;}
.bt text{color:#252525;margin-right: 20rpx }
.price{display: block;color: #d74a3c}
.zt{color:#d74a3c }
.hh view{display: inline-block}
.sl{float: right;margin-right: 30rpx;font-size: 36rpx;}
.order_info{width: 90%;margin: 0 auto;font-size:34rpx ;padding-bottom: 100rpx}

.edit{width: 94%;display: flex;align-items: center;justify-content: flex-end;font-size: 34rpx;margin: 15rpx auto 0}
.edit view{ border: 1px solid #eee;margin-left: 30rpx;padding: 10rpx 15rpx ;border-radius:3px}
.active{color:#d74a3c;border-color:#d74a3c!important; }