<!--index.wxml-->
<view class="container">
  <view class='word_box'>
    <image src='/images/word1.png' class='word1'></image>
    <image src='/images/word2.png' class='word2'></image>
  </view>
  <view class="list-wrapper">
    <view class="list-top">
      <view data-num="1" class="list-menu  list-menu1 {{_num==1?'cur':''}}" bindtap="menuClick">开始定制</view>
      <view data-num="2" class="list-menu list-menu2 {{_num==2?'cur':''}}" bindtap="menuClick">案 例</view>
      <view data-num="3" class="list-menu list-menu3 {{_num==3?'cur':''}}" bindtap="menuClick">个人中心</view>
    </view>
  </view>
  <!--  
  <view class='history' bindtap='history'>
   <text class='iconfont icon-work'></text>
  </view> -->
   <view>
    <!--引导视频-->
    <view class="lead_video" hidden='{{video_hidden}}'>
           <video id="myVideo"  custom-cache="{{false}}" src="{{video_src}}" binderror="videoErrorCallback" danmu-list="{{danmuList}}"  enable-danmu="false" danmu-btn="false"  controls show-center-play-btn="true" bindended="video_end" ></video>
    </view>
  </view>
</view>