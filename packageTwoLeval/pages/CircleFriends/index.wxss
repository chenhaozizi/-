/* packageTwoLeval/pages/myInMoney/index.wxss */
@import '/lib/common.wxss';
page{background: #EBEBEB}
.head{background: url(data:image/png;base64,) no-repeat center;height: 200rpx;width: 100%;background-size: cover;box-sizing: border-box;position: fixed;top:0;z-index: 99}
.loading{
  padding: 10rpx;
  text-align: center;
 
}
.loadeimg{  animation: rotate 1s linear infinite;width: 64rpx;height: 64rpx;position: relative;top:32rpx;right:10rpx}
@keyframes rotate
{
from { transform: rotate(0deg);}
           
to { transform: rotate(360deg);
            -webkit-transform: rotate(360deg);
            -moz-transform: rotate(360deg);
            -o-transform: rotate(360deg);
            -ms-transform: rotate(360deg);}
}

.loading:before{
  display: inline-block;
  margin-right: 5rpx;
  vertical-align: middle;
  content: '';
  width: 40rpx;
  height: 40rpx;
  /* background: url(../../../images/load.png) no-repeat;
  background-size: contain; */
 
}
.loading.complete:before{
  display: none;
}
.w50{width: 50%}