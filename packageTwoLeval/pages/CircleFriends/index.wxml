<view>
<!--头部  -->
  <view class='head flex color-ff'>
      <view class='text-center '>
          <view class='font-sm mt5 mb15'>我的朋友圈</view>
          <view class='font-80'>10<text class='font-xs'>人</text></view>
      </view>
      <view class='text-center '>
          <view class='font-sm mt5 mb15'>拥有的二级分销商</view>
          <view class='font-80'>2<text class='font-xs'>人</text></view>
      </view>
  </view>
<!-- 列表  -->
 <scroll-view scroll-y="true" bindscrolltolower="searchScrollLower" style='margin-top:220rpx;margin-bottom:40rpx;height:calc(100vh)'>
    <view wx:for="{{searchSongList}}" wx:key="unique"  data-data="{{item}}" class='list mt20'>
      <view class='list-item pos-rel'>
        <image class='pos-ab'  src='http://thirdwx.qlogo.cn/mmopen/vi_32/RcaSh5gLHQoK9ZicIxfrkPMSQhrRzMdibqTH4fI6ebdPgRnKa37gdkjn5SbT2n5IBwAFgfmuQrllWQsrl91cbRaA/132'></image>
