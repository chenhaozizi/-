          <view class='lin50 mt20'>
            <view class='font-md'>用户：张三{{index}}</view>
            <view class='font-sm'>订单量：3</view>
          </view>
          <view class='font-md color-y font-bold'>
          <view><i class="iconfont icon-user font-36 lin128"></i>普通用户</view>
          </view>
        </view>
      </view>
    </view>
